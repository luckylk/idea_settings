<keymap version="1" name="Default for XWin copy Proper Redo" parent="Default for XWin">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="AutoIndentLines" />
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="alt slash" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EmacsStyleIndent">
    <keyboard-shortcut first-keystroke="ctrl alt i" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="HippieCompletion" />
  <action id="OnlineSearch.EditorActions.ShowSearchEnginesAction">
    <keyboard-shortcut first-keystroke="ctrl alt 1" />
  </action>
  <action id="OptimizeImports">
    <keyboard-shortcut first-keystroke="ctrl alt o" />
    <mouse-shortcut keystroke="control button1" />
  </action>
</keymap>